{
  "name": "supertest-express-demo",
  "version": "1.0.0",
  "description": "Small Express API with SuperTest + Mocha + Chai tests running in-memory",
  "main": "server.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "mocha --recursive --exit 'test/**/*.test.js'",
    "test:report": "mocha --reporter mochawesome 'test/**/*.test.js'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3"
  }
}
